@model RTSafe.RTDP.Permission.Models.Menu

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Menu</legend>

    <div class="display-label">ParentMenu</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.ParentMenu.Name)
    </div>

    <div class="display-label">Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">Description</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>

    <div class="display-label">EntryUrl</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.EntryUrl)
    </div>
